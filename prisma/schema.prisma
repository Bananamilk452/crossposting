// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/@prisma-app/client"
  moduleFormat = "esm"
  importFileExtension = "ts"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model AuthState {
  key   String @id
  state String

  @@map("auth_states")
}

model AuthSession {
  key     String @id
  session String

  @@map("auth_sessions")
}
